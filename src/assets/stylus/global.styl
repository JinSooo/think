body, html, #app, #bgCanv
  width 100%
  height 100%
  overflow auto
// element-ui样式
.el-timeline-item__timestamp
  font-size 12px
.el-timeline-item__content
  color #eee
.el-container
  height 100%
.el-pagination
  text-align center
  margin-top 10px
.el-card, .el-card__body
  height 100%
.el-card__header
  padding-left 0
.is-never-shadow
  border 1px solid #F9F9F9
// 滚动条样式
::-webkit-scrollbar
  width 8px
::-webkit-scrollbar:horizontal
  height 8px
::-webkit-scrollbar-thumb
  border-radius 4px
  background-color rgba(0, 0, 0, 0.2)
::-webkit-scrollbar-track
  background-color transparent
// 省略字符
.over_word
  white-space nowrap
  overflow hidden
  text-overflow ellipsis
// 分割线
.divider
  margin 24px 0
  height 1px
  background-image linear-gradient(to right, white, rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45), white)
// 分割线
.divider_vertical
  margin 5px auto
  width 1px
  height 40px
  background-image linear-gradient(to top, white, rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45), rgba(0, 0, 0, 0.45), white)
// 清除浮动
.clearfix::after
  display block
  height 0
  content ''
  clear both
  visibility hidden