body, html, #app, #bgCanv
  width 100%
  height 100%
  overflow auto
.el-timeline-item__timestamp
  font-size 12px
.el-timeline-item__content
  color #eee
.el-container
  height 100%
.el-pagination
  text-align center
  margin-top 10px
.el-card, .el-card__body
  height 100%
.el-card__header
  padding-left 0
.is-never-shadow
  border 1px solid #F9F9F9
::-webkit-scrollbar
  width 8px
::-webkit-scrollbar:horizontal
  height 8px
::-webkit-scrollbar-thumb
  border-radius 4px
  background-color rgba(0, 0, 0, 0.2)
::-webkit-scrollbar-track
  background-color transparent